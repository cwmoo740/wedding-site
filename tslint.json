{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-no-multiline-js": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-snake-case"
    ]
  }
}
